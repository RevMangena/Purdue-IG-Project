pipeline
{
	agent any
	stages
	{
		stage('Code Checkout')
		{
			steps
			{
				git 'https://github.com/RevMangena/Purdue-IG-Project.git'
			}
		}
		
		stage('1.Compile')
		{
			steps
			{
				sh 'mvn compile'
			}
		}

		stage('2.Test')
		{
			steps
			{
				sh 'mvn test'
			}
		}

		stage('Package')
		{
			steps
			{
				sh 'mvn package'
			}
		}
   }
}
